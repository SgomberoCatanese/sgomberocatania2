/* Animations */
@keyframes fadeInUp{
    from{opacity:0;transform:translateY(30px)}
    to{opacity:1;transform:translateY(0)}
}

[data-aos="fade-up"]{
    opacity:0;
    transform:translateY(30px);
    transition:all 0.6s ease-out
}

[data-aos="fade-up"].aos-animate{
    opacity:1;
    transform:translateY(0)
